{
    "name": "akrabat/apigility-xml",
    "type": "library",
    "description": "Apigility module providing XML content-negotiation features",
    "keywords" : [
		"api",
		"apigility",
		"module",
		"zf3",
		"xml",
        "zend",
		"negotiation",
        "content-negotiation"
	],
    "homepage": "https://github.com/akrabat/apigility-xml",
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^2.0",
        "zendframework/zend-serializer": "^2.7.7 || ^3.0.1",
        "zendframework/zend-stdlib": "^2.7.7 || ^3.0.1",
        "zendframework/zend-xml2json": "^3.0",
        "zfcampus/zf-apigility": "^1.3",
        "zfcampus/zf-content-negotiation": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Akrabat\\ApigilityXml\\": "src/"
        }

    },
    "extra": {
        "zf": {
            "component": "Akrabat\\ApigilityXml"
        }
    }
}
